{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "vrc-worlds-manager",
  "version": "0.1.0",
  "identifier": "com.raifaworks.vrc-worlds-manager",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "VRC Worlds Manager",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI5NDRGQjVEOTcwOUE4RDUKUldUVnFBbVhYZnRFS2R6OVVHalozZEdNTVhCV21ZNDZhSFpMZ3dYRHAwd3d3UmhyYnBUMEZJSHgK",
      "endpoints": ["https://releases.raifaworks.com/vrc-worlds-manager/latest.json"]
    }
  }
}
